a=int(input("enter any number :"))
def divby2and7(a):
    if a%2==0 and a%7==0:
        print("it is div by 2 and three")
    elif a%2==0:
        print("it is div only by 2 ")
    elif a%7==0:
        print("it is div by only 7")
divby2and7(a)

a = input("Enter string: ")

def empty(a):
    if a.strip() == "":
        print("This is empty")
    else:
        print("This is not empty")

empty(a)
  
a=int(input("enter any number find the digit :"))
def digit(a):
    if a>0 and a <10:
        print("it is single digit ")
    elif a>9 and a<100:
        print("it is 2 digit number")
    elif a>100 and a<1000:
        print("it is 3 digit number ")
    else:
        print("the limit is upto only 3 digit")
digit(a)

 
a=int(input("enter any number :"))
b=int(input("enter another number :"))
c=a+b
def sumoftwo(a,b):
    if c%2==0:
        print("it is even number ")
    else:
        print("it is odd number ")
sumoftwo(a,b)