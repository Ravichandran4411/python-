def add(*num):
    total = 0
    for i in num:
        total += i
    print(total)
add(10, 20, 30, 40, 59)


age=int(input("enter your age to find your eligibility :"))
def eligibility(age):
    if age>18:
        print("you are eligible")
    else:
        print("you are not eligible")
eligibility(age)


num=int(input("enter any number : "))
def oddeven(num):
    if num%2==0:
        print("it is even number")
    else:
        print("it is odd number")
oddeven(num)
        
num1=int(input("enter the a number : "))
num2=int(input("enter the b number : "))
def compare(num1,num2):
    if num1>num2:
        print("num1 is greater than num2 ")
    else:
        print("num2 is greater than num1 ")
compare(num1,num2)


def triangle(a, b, c):
    if a == b == c:
        return "Equilateral triangle"
    elif a == b or b == c or a == c:
        return "Isosceles triangle"
    else:
        return "Scalene triangle"

s1 = float(input("Enter s 1: "))
s2 = float(input("Enter s 2: "))
s3 = float(input("Enter s 3: "))
print(triangle(s1,s2,s3))



def grade(mark):
    if mark >= 8:
        print("A+")
    elif mark >= 6:
        print("A")
    elif mark >= 4:
        print("B+")
    else:
        print("B")
grade(3)        

def marks(m):
    for mark in m:
        grade(mark)
listt = [4.5,3.7,8.9,4.3]
marks(listt)


